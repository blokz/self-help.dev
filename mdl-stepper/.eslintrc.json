{
  "env": {
    "browser": true
  },
  "extends": "google",
  "globals": {
    "componentHandler": false
  },
  "rules": {
    "linebreak-style": 0,
    "space-before-function-paren": 0,
    "max-len": [
      1,
      100,
      2
    ],
    "valid-jsdoc": [
      1,
      {
        "requireReturnDescription": false
      }
    ]
  }
}